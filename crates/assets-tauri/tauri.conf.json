{
  "productName": "RustyAssets",
  "version": "0.1.0",
  "identifier": "com.rustyassets.app",
  "build": {
    "beforeDevCommand": "./start-dev.sh",
    "beforeBuildCommand": "./build-ui.sh",
    "devUrl": "http://localhost:5173",
    "frontendDist": "ui/dist",
    "withGlobalTauri": false
  },
  "app": {
    "windows": [
      {
        "title": "RustyAssets",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all"
  },
  "plugins": {
    "updater": {
      "active": false
    }
  }
}